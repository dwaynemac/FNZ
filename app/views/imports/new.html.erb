<h1>New import</h1>

<div id="valid-headers">
  <h3><%= t('imports.new.valid_headers') %></h3>
  <%- Import::VALID_HEADERS.each_value do |vh| -%>
    <%= vh %> - 
  <%- end -%>
</div>

<%- semantic_form_for @import,:html => {:multipart => true} do |f| -%>
  <%= f.error_messages %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%- f.buttons do -%>
    <%= f.commit_button %>
  <%- end -%>
<%- end -%>

<%= link_to 'Back', imports_path %>